        <button type="button" id="catalogSynchronizationProducts_right_All"><i class="icon icon-forward-all"></i></button>
        <button type="button" id="catalogSynchronizationProducts_right_Selected"><i class="icon icon-forward"></i></button>
        <button type="button" id="catalogSynchronizationProducts_left_Selected"><i class="icon icon-backward"></i></button>
        <button type="button" id="catalogSynchronizationProducts_left_All"><i class="icon icon-backward-all"></i></button>
    </div>
    <div class="right">
        <select name="to" id="catalogSynchronizationProducts_to" size="12" multiple="multiple"></select>
    </div>
    <div class="clear"></div>
</div>